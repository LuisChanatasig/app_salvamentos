@* Views/Analisis/_ValoresComerciales.cshtml *@
<h6 class="text-uppercase text-muted mt-4 mb-3">Valores Comerciales</h6>
<div class="row g-2">
    <div class="col-md-3">
        <label class="form-label fw-semibold">Patio Tuerca</label>
        <input name="ValorTuerca" type="number" step="0.01" class="form-control form-control-sm" />
        <input name="RespaldoTuerca" type="file" class="form-control form-control-sm mt-1" />
    </div>
    <div class="col-md-3">
        <label class="form-label fw-semibold">Patio Marketplace</label>
        <input name="ValorMarketplace" type="number" step="0.01" class="form-control form-control-sm" />
        <input name="RespaldoMarketplace" type="file" class="form-control form-control-sm mt-1" />
    </div>
    <div class="col-md-3">
        <label class="form-label fw-semibold">Patio Hugo Vargas</label>
        <input name="ValorHugoVargas" type="number" step="0.01" class="form-control form-control-sm" />
        <input name="RespaldoHugoVargas" type="file" class="form-control form-control-sm mt-1" />
    </div>
    <div class="col-md-3">
        <label class="form-label fw-semibold">Patio Otros</label>
        <input name="ValorOtros" type="number" step="0.01" class="form-control form-control-sm" />
        <input name="RespaldoOtros" type="file" class="form-control form-control-sm mt-1" />
    </div>
</div>
